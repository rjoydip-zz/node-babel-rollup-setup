{
    "parser": "babel-eslint",
    "extends": [
        "standard"
    ],
    "env": {
        "es6": true,
        "node": true,
        "commonjs": true
    },
    "plugins": [
       
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        },
        "sourceType": "module"
    },
    "rules": {
        "space-before-function-paren": 0,
        "react/jsx-boolean-value": 0
    }
}